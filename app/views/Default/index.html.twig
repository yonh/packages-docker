    {% if packages_conf.contact_email is not empty %}
    <p>If you should have access, contact us.
      <span class="contact">{{ packages_conf.contact_email }}</span>
    </p>
    {% endif %}
    <a href="{{ path('packages_index') }}" class="btn btn-primary">Available Packages</a>
{% endblock %}
